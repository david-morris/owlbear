{:dependencies [[binaryage/oops "0.7.1"]
                [cljstache "2.0.6"]]
 :source-paths ["src/cljs"]
 :builds       {:lib
                {:target    :node-library
                 :exports   {:htmlForwardSlurp owlbear.parser.html.tagedit.slurp/forward-slurp
                             :htmlForwardBarf owlbear.parser.html.tagedit.barf/forward-barf}
                 :output-to "out/cljs/owlbear.js"}
                :test
                {:target    :node-test
                 :output-to "out/cljs/owlbear-test.js"
                 :ns-regexp "-test$"
                 :autorun true}}}